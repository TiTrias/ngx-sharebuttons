{
  "name": "@ngx-share/demo",
  "version": "7.1.4",
  "description": "Angular Share Buttons",
  "homepage": "http://github.com/MurhafSousli/ngx-sharebuttons",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/MurhafSousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-sharebuttons.git"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "share-links",
    "share-buttons",
    "sharebuttons",
    "share",
    "buttons",
    "facebook",
    "twitter",
    "pinterest",
    "tumblr",
    "stumbleupon",
    "googleplus",
    "linkedin",
    "reddit",
    "telegram",
    "vk",
    "copy",
    "print",
    "email"
  ],
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-sharebuttons/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-demo": "ng serve demo --host 0.0.0.0",
    "start-demo-aot": "ng serve demo --host 0.0.0.0 --aot",
    "build-demo": "ng build demo --prod --base-href https://murhafsousli.github.io/ngx-sharebuttons/",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc-core": "./node_modules/.bin/compodoc -p projects/core/tsconfig.lib.json -d docs/core",
    "compodoc-button": "./node_modules/.bin/compodoc -p projects/button/tsconfig.lib.json -d docs/button",
    "compodoc-buttons": "./node_modules/.bin/compodoc -p projects/buttons/tsconfig.lib.json -d docs/buttons",
    "sass-ts": "tsc .config/styles.ts --lib es7 && node .config/styles.js projects/button/src/lib/themes dist/button/themes",
    "sass": "node .config/styles.js projects/button/src/lib/themes dist/button/themes",
    "init-build": "npm run build-prod-core && npm run version && npm run link-core && npm run build-prod-button && npm run version && npm run link-button && npm run build-prod-buttons && npm run version && npm run link-buttons && npm run sass && npm run version",
    "build-all": "npm run build-prod-core && npm run build-prod-button && npm run build-prod-buttons && npm run sass && npm run version",
    "build-core": "ng build core",
    "build-button": "ng build button",
    "build-buttons": "ng build buttons",
    "build-prod-core": "ng build core --prod",
    "build-prod-button": "ng build button --prod",
    "build-prod-buttons": "ng build buttons --prod",
    "link-core": "npm link dist/core && npm link @ngx-share/core",
    "link-button": "npm link dist/button && npm link @ngx-share/button",
    "link-buttons": "npm link dist/buttons && npm link @ngx-share/buttons",
    "publish-all": "node .config/publish.js",
    "version": "node .config/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@ngx-progressbar/core": "^5.3.1",
    "@ngx-progressbar/http": "^5.3.1",
    "@ngx-progressbar/router": "^5.3.1",
    "@ngx-pwa/local-storage": "^8.2.4",
    "core-js": "^3.6.4",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.17.1",
    "ngx-highlightjs": "^4.0.2",
    "ngx-scrollbar": "^7.1.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/build-ng-packagr": "^0.803.23",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.1.7",
    "ansi-colors": "^4.1.1",
    "codelyzer": "~5.2.1",
    "cssnano": "^4.1.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^5.7.1",
    "node-sass": "^4.13.1",
    "postcss-strip-inline-comments": "^0.1.5",
    "protractor": "~5.4.1",
    "replace-ext": "^1.0.0",
    "ts-node": "~8.6.2",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.20.1",
    "typescript": "^3.4.5"
  }
}
